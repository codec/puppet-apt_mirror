# mirror <%= name -%>
<% release.each do |this_release| -%>
<% arch.each do |this_arch| %>
deb<%= "-#{this_arch}" if this_arch != architecture %> http://<%= mirror -%>/<%= os -%> <%= this_release -%> <%= components.join(' ') -%>
<% end -%>
<% if source -%>
deb http://<%= mirror -%>/<%= os -%> <%= this_release -%> <%= components.join(' ') -%>
<% end -%>
<% end -%>
